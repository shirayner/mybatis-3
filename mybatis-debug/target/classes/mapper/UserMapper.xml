<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ray.study.mybatis.debug.mapper.UserMapper">

  <insert id="insert" parameterType="com.ray.study.mybatis.debug.entity.User" useGeneratedKeys="true" keyProperty="id">
      INSERT INTO
      `user`(`name`,`age`, creation_date, last_update_date)
      VALUES (#{name},#{age},#{creationDate},#{lastUpdateDate})
  </insert>

    <select id="findById" resultType="com.ray.study.mybatis.debug.entity.User">
        select
          id, `name`, age, creation_date, last_update_date
        from `user`
        where id=#{id}
    </select>

</mapper>
